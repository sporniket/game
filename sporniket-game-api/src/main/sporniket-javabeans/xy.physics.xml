<?xml version="1.0" encoding="UTF-8"?>
<BeanSet xmlns="http://schema.sporniket-studio.com/model/set/javabean">
	<annotation>
		<author>David SPORN</author>
		<licencenotice>LGPL v3</licencenotice>
		<version>15.05.01-SNAPSHOT</version>
	</annotation>
	<package name="com.sporniket.libre.game.api.types.xy.physics">
		<annotation>
			<summary>Collection of types to model a physic engine on the xy plane.</summary>
			<description>'x' is oriented from left to rigth, 'y' is oriented from top to bottom, like a device screen</description>
		</annotation>
		<bean name="PsaPoint">
			<annotation>
				<summary>P-osition,S-peed,A-cceleration point.</summary>
				<description>A physical point.</description>
			</annotation>
			<property name="position" type="java:com.sporniket.libre.game.api.types.xy.geometry.Point" mode="basic" />
			<property name="speed" type="java:com.sporniket.libre.game.api.types.xy.geometry.Point" mode="basic" />
			<property name="acceleration" type="java:com.sporniket.libre.game.api.types.xy.geometry.Point" mode="basic" />
		</bean>
	</package>
</BeanSet>