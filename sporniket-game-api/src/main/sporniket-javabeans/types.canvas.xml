<?xml version="1.0" encoding="UTF-8"?>
<BeanSet xmlns="http://schema.sporniket-studio.com/model/set/javabean">
	<annotation>
		<author>David SPORN</author>
		<licencenotice>LGPL v3</licencenotice>
		<version>15.05.01-SNAPSHOT</version>
	</annotation>
	<package name="com.sporniket.libre.game.api.types.canvas">
		<bean name="Bounds">
			<annotation>
				<summary>A box described by its limits.</summary>
				<description>A point is inside the box if left&lt;=px&lt;=right and
					top&lt;=py&lt;bottom.</description>
			</annotation>
			<property name="left" type="java:Integer" mode="basic" />
			<property name="right" type="java:Integer" mode="basic" />
			<property name="top" type="java:Integer" mode="basic" />
			<property name="bottom" type="java:Integer" mode="basic" />
		</bean>
	</package>
</BeanSet>